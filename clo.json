{
  "version": 1,
  "cmd": {
    "auth": [
      "username",
      "password"
    ],
    "get-data": [
      "product-type_!=store-page,account-page,wishlist-page,details,api-products-v1,api-products-v2",
      "media=game_,movie",
      "id...",
      "missing",
      "updated",
      "deny-ids-file",
      "verbose"
    ],
    "extract": [
      "media=game_,movie",
      "property...=title,developer,publisher,image,box-art,logo,icon,background,galaxy-background,screenshots,includes-games"
    ],
    "get-images": [
      "image-type_!=image,box-art,logo,icon,background,galaxy-background,screenshots",
      "media=game_,movie",
      "id...",
      "all"
    ],
    "get-files": [
      "os=current_,windows,macos,linux",
      "lang=en_,ru"
    ],
    "info": [
      "media=game_,movie",
      "id!_...",
      "images"
    ],
    "list": [
      "product-type_!=store-products,account-products,wishlist-products,details,api-products-v1,api-products-v2",
      "media=game_,movie",
      "property...=developer,publisher,image,box-art,logo,icon,background,galaxy-background,screenshots,rating,includes-games",
      "modified"
    ],
    "search": [
      "media=game_,movie",
      "text_",
      "title",
      "developer",
      "publisher",
      "genres",
      "image-id"
    ],
    "scrub-data": [
      "media=game_,movie",
      "fix"
    ],
    "sync": [
      "media=game_,movie",
      "no-data",
      "images",
      "screenshots",
      "all"
    ],
    "wishlist": [
      "media=game_,movie",
      "add...",
      "remove..."
    ]
  },
  "help": {}
}