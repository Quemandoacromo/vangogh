{
  "version": 1,
  "cmd": {
    "auth": [
      "username",
      "password"
    ],
    "get-data": [
      "product-type*^={remote-product-types}",
      "media={media^}",
      "id&",
      "slug",
      "read-ids",
      "missing",
      "updated",
      "deny-ids-file",
      "verbose"
    ],
    "extract": [
      "media={media^}",
      "property&={extracted-properties}"
    ],
    "get-images": [
      "image-type*^={image-types}",
      "media={media^}",
      "id&",
      "slug",
      "read-ids",
      "missing"
    ],
    "get-videos": [
      "id&",
      "slug",
      "read-ids",
      "all"
    ],
    "get-downloads": [
      "id^&",
      "slug",
      "media={media^}",
      "operating-system&={operating-systems^}",
      "download-type&={download-types^}",
      "language-code&={language-codes^}",
      "read-ids",
      "extras",
      "all"
    ],
    "info": [
      "media={media^}",
      "id^&",
      "read-ids",
      "slug",
      "all-text",
      "images",
      "video-id"
    ],
    "list": [
      "product-type^*={local-product-types}",
      "media={media^}",
      "property&={all-properties}",
      "modified"
    ],
    "owned": [
      "id^&",
      "slug",
      "read-ids"
    ],
    "digest": [
      "property^={digestible-properties}"
    ],
    "search": [
      "media={media^}",
      "{searchable-properties^&}"
    ],
    "scrub-data": [
      "media={media^}",
      "fix"
    ],
    "sync": [
      "media={media^}",
      "no-data",
      "images",
      "screenshots",
      "videos",
      "all",
      "since-hours-ago"
    ],
    "summary": [
      "media={media^}",
      "since-hours-ago"
    ],
    "tag": [
      "operation^=create,delete,edit,add,remove",
      "tag-name",
      "id"
    ],
    "wishlist": [
      "media={media^}",
      "add&",
      "remove&"
    ]
  },
  "help": {}
}