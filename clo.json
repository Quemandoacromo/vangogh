{
  "version": 1,
  "cmd": {
    "auth": [
      "username",
      "password"
    ],
    "get-data": [
      "product-type!_={remote-product-types}",
      "media={media_}",
      "id...",
      "read-ids",
      "missing",
      "updated",
      "deny-ids-file",
      "verbose"
    ],
    "extract": [
      "media={media_}",
      "property...={extracted-properties}"
    ],
    "get-images": [
      "image-type!_={image-types}",
      "media={media_}",
      "id...",
      "read-ids",
      "all"
    ],
    "get-videos": [
      "id...",
      "read-ids",
      "all"
    ],
    "get-files": [
      "os=current_,windows,macos,linux",
      "lang=en_,ru"
    ],
    "info": [
      "media={media_}",
      "id_...",
      "read-ids",
      "all-text",
      "images",
      "video-id"
    ],
    "list": [
      "product-type_!={local-product-types}",
      "media={media_}",
      "property...={all-properties}",
      "modified"
    ],
    "owned": [
      "id_...",
      "read-ids"
    ],
    "digest": [
      "property_={digestible-properties}"
    ],
    "search": [
      "media={media_}",
      "{searchable-properties_...}"
    ],
    "scrub-data": [
      "media={media_}",
      "fix"
    ],
    "sync": [
      "media={media_}",
      "no-data",
      "images",
      "screenshots",
      "all",
      "since-hours-ago"
    ],
    "summary": [
      "media={media_}",
      "since-hours-ago"
    ],
    "wishlist": [
      "media={media_}",
      "add...",
      "remove..."
    ]
  },
  "help": {}
}