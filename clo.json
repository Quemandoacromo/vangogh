{
  "version": 1,
  "cmd": {
    "auth": [
      "username",
      "password"
    ],
    "get-data": [
      "product-type!_={remote-product-types}",
      "media={_media}",
      "id...",
      "missing",
      "updated",
      "deny-ids-file",
      "verbose"
    ],
    "extract": [
      "media={_media}",
      "property...={extracted-properties}"
    ],
    "get-images": [
      "image-type!_={image-types}",
      "media={_media}",
      "id...",
      "all"
    ],
    "get-videos": [
      "id...",
      "all"
    ],
    "get-files": [
      "os=current_,windows,macos,linux",
      "lang=en_,ru"
    ],
    "info": [
      "media={_media}",
      "id!_...",
      "images",
      "video-id"
    ],
    "list": [
      "product-type_!={local-product-types}",
      "media={_media}",
      "property...={all-properties}",
      "modified"
    ],
    "digest": [
      "property_={one-to-many-properties}"
    ],
    "search": [
      "media={_media}",
      "{_searchable-properties}"
    ],
    "scrub-data": [
      "media={_media}",
      "fix"
    ],
    "sync": [
      "media={_media}",
      "no-data",
      "images",
      "screenshots",
      "all"
    ],
    "wishlist": [
      "media={_media}",
      "add...",
      "remove..."
    ]
  },
  "help": {}
}